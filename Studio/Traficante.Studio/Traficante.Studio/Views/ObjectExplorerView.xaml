<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Traficante.Studio.Views.ObjectExplorerView">
  <DockPanel>
    <StackPanel VerticalAlignment="Top" Orientation="Horizontal">
        <Menu>
          <MenuItem Header="_Connect">
            <MenuItem Header="_SqlServer" Name="SqlServer" Command="{Binding Connect}" CommandParameter="SqlServer"/>
            <MenuItem Header="_MySql" Name="MySql" Command="{Binding Connect}" CommandParameter="MySql"/>
          </MenuItem>
        </Menu>
    </StackPanel>
  </DockPanel>
  
</UserControl>
